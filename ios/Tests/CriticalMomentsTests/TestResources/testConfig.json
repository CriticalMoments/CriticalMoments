{
    "configVersion": "v1",
    "appId": "com.apple.dt.xctest.tool",
    "themes": {
        "defaultThemeName": "namedDefault",
        "namedThemes": {
            "namedDefault": {
                "bannerBackgroundColor": "#ff0000",
                "bannerForegroundColor": "#00ff00",
                "fontScale": 1.1,
                "fontName": "AvenirNext-Regular",
                "boldFontName": "AvenirNext-Bold",
                "scaleFontForUserPreference": false
            }
        }
    },
    "conditions": {
        "namedConditions": {
            "invalidCondition": "fake_var > 6",
            "trueCondition": "true",
            "falseCondition": "false"
        }
    },
    "actions": {
        "namedActions": {
            "alertAction": {
                "actionType": "alert",
                "actionData": {
                    "title": "Alert title"
                }
            },
            "reviewAction": {
                "actionType": "review_prompt"
            }
        }
    }
}
